# Format: filename_weight (weight = 1000 if omitted)
#   individual times are printed as is, but accumulated time is weighed
#   by (weight / 1000)

test_extra="\
  compat\
  galois\
  ploth\
"

test_basic="\
  objets\
  analyz\
  number\
  polyser\
  linear\
  elliptic\
  sumiter\
  graph\
  program\
  trans\
  nfields_200\
"

top_test_extra="test-`echo $test_extra | sed -e 's/ \\([^ ]\\)/ test-\\1/g'`"
top_dotest_extra="dotest-`echo $test_extra | sed -e 's/ \\([^ ]\\)/ dotest-\\1/g'`"
