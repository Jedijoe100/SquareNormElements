default(realprecision,38);
v=[3,4,3.1,1/2,x^2+1, Mod(x,x^2+1),ffgen(3^5,'t),quadgen(5),2+O(3^3),Mod(2,3)];
{
for (i=1,#v,
  for(j=1,#v,
    print("* ",[i,j]);
    print(iferr(v[i]/v[j],E,E));
    print(iferr(v[i]\v[j],E,E));
    print(iferr(v[i]%v[j],E,E));
    print(iferr(v[i]\/v[j],E,E));
  )
)
}
\\ [2,3], Mat(2);
