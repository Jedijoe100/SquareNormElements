setrand(1);bnf=bnfinit(x^2-x-57);
test(m)=
{
  print(bnrisconductor(bnf,m), ", ", bnrclassno(bnf,m), ", ", bnrdisc(bnf,m));
}
test([[25,13;0,1],[1,1]])
test([[25,13;0,1],[1,0]])
test([[5,3;0,1],[1,0]])
test([5,3;0,1])

m=[[5,3;0,1],[1,0]];
H=Mat(2);
bnr=bnrinit(bnf, idealstar(bnf,m,2));
f=[bnrclassno,bnrdisc,bnrconductor];
{
for (i=1,#f,
  print(f[i](bnr));
  print(f[i](bnr,H));
  print(f[i](bnf,m));
  print(f[i](bnf,m,H));
)
}
bnrdisc(bnf,m,H,1)
bnrdisc(bnf,m,H,2)
bnrdisc(bnf,m,H,3)
bnrdisc(bnf,[5,3;0,1],H,2)
bnrdisc(bnf,[5,3;0,1],H,3)
