f(x) = 1/(exp(x)-1) - exp(-x)/x;
F = truncate( f(t + O(t^7)) );
g(x) = if (x > 1e-18, f(x), subst(F,t,x));
check(intnum(x = 0, [oo,1],  f(x)), Euler)
check(intnum(x = 0, [oo,1],  g(x)), Euler)
