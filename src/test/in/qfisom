checkauto(M)=
{
  my(G=qfauto(M));
  for(j=1,#G[2],my(Q=G[2][j]);
      if(M!=Q~*M*Q ,error(v)));
  print(G[1]);
}
checkisom(M,N,f)=
{
  my(Q=qfisom(if(f,qfisominit(M),M),N));
  if(Q==0 || M!=Q~*N*Q ,error(N),print("OK"));
}
