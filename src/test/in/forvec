default(realprecision,38);
V=vector(3,i,[1.5,3]);
forvec(v=V,print(v))
print()
V=vector(3,i,[1.5,4]);
forvec(v=V,print(v),1)
print()
forvec(v=V,print(v),2)
V=vector(3,i,[1,3]);
forvec(v=V,print(v))
print()
forvec(v=V,print(v),1)
print()
forvec(v=V,print(v),2)
forvec(v=[[1,2],[0,1]],print(v),1)
forvec(v=[[1,2],[0,1]],print(v),2)

forvec(v=[],print(v))
forvec(v=[1],)
forvec(v=[[1]],)
forvec(v=[[1,I]],)
