myprintsep(s,v[..])=if(#v>0,print1(v[1]);for(i=2,#v,print1(s,v[i])));print();
print(myprintsep)
?myprintsep
print1(":");myprintsep()
print1(":");myprintsep(", ")
myprintsep(", ",a,b,c,d)
myprintsep(", ",a,,c,)
myprintsep(,a,b,c,d)
