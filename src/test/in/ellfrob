{
  for (b = 3,25,
    my(E=ellinit([3,b]));
    forprime(p=5,17,
      if ((4+b^2)%p == 0, next);
      my (F = ellpadicfrobenius(E,p,20)[,1]~);
      print([b,p,lift(F)])
    )
  )
}

e=ellinit([1,-1,1,-1,-14]);
E=ellinit([0,0,0,-e.c4/48,-e.c6/864]);
P=[1,-1/4;0,1];
{
  forprime(p=3,40,if(p==17,next);
  A=ellpadicfrobenius(e,p,10);
  B=ellpadicfrobenius(E,p,10);
  if(P*B!=A*P,error(p)));
}
