default(realprecision,38);
v = [[1,2], [4,1], [3,4], [1,3/2], [5/2,1.]]; x = [1,2]; y = [5,2];
w = vecsort(v)
vecsearch(w, x)
vecsearch(w, y)

K = [ 1, 2, 3, [2,1], [3,1], (x,y)->sign(x[1] - y[1]) ];
{
  for (i = 1, #K,
    iferr (w = vecsort(v, K[i]);
           print(w);
           print(vecsearch(w, x, K[i]));
           print(vecsearch(w, y, K[i])),
           E, print(E))
  )
}
